\ufeff<?xml version="1.0" encoding="utf-8"?>
<toolbox>
		<actionspanelsupplement>
			<string name="attachSound" object="Sound" text=".attachSound(% idNombre=&quot;&quot; %)">
				<parametersdialog>
					<label value="ID Nombre:" control="ATTACH_SND_EXPR" align="right" id="ATTACH_SND_LABEL"/>
					<expression id="ATTACH_SND_EXPR" size="60" prompt="string"/>
				</parametersdialog>
			</string>
			<string name="setRGB" object="Color" text=".setRGB(% 0xRRGGBB %)">
				<parametersdialog>
					<label value="Color:" control="COLORCHIP"/>
					<colorchip id="COLORCHIP" format="hex"/>
				</parametersdialog>
			</string>
			<string name="load" object="[TextField.StyleSheet]" text=".load(% url %)">
				<parametersdialog>
					<label value="URL:" control="URL_EXPRESSION" align="right" id="URL_LABEL"/>
					<expression id="URL_EXPRESSION" size="60" prompt="URL"/>
				</parametersdialog>
			</string>
			<string name="setSelectColor" object="TextSnapshot" text=".setSelectColor(% color %)">
				<parametersdialog>
					<label value="Color:" control="COLORCHIP"/>
					<colorchip id="COLORCHIP" format="hex"/>
				</parametersdialog>
			</string>
			<string name="upload" object="[flash.net.FileReference]" text=".upload(% url:String %)">
				<parametersdialog>
					<label value="URL:" control="URL_TEXT"/>
					<textbox id="URL_TEXT" size="100" prompt="url"/>
				</parametersdialog>
			</string>
		</actionspanelsupplement>
</toolbox>
