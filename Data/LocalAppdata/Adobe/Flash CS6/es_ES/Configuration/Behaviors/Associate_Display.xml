<flash_behavior version="1.0">
	<behavior_definition dialogID="AssociateDisplayDialog" defaultEvent="load" events="load" category="Media" authoringEdition="pro" name="Visualización asociada">
		<properties>
			<property id="target" default="_parent"/> 
		</properties>
		<dialog id="AssociateDisplayDialog" title="Visualización asociada" buttons="accept, cancel">
			<vbox>
				<label value="Seleccione una instancia del componente visualización:" control="target"/> 
				<targetlist id="target" class="mx.controls.MediaDisplay" required="true" requiredname="target"/>
			</vbox> 
		</dialog>
		<actionscript>
			<![CDATA[
	// AssociateDisplay Behavior
	this.associateDisplay($target$);
	// End AssociateDisplay Behavior
			]]>
		</actionscript>
	</behavior_definition>
</flash_behavior>
